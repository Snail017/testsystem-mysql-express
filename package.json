{
  "name": "testsystem-mysql-express",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve  --open",
    "build": "vue-cli-service build",
    "app": "node server/app.js",
    "test:unit": "vue-cli-service test:unit"
  },
  "bin": "server/app.js",
  "pkg": {
    "assets": [
      "server/key/**/*"
    ],
    "key": "server/key/*.key"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "babel-plugin-component": "^1.1.1",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.5.3",
    "compression-webpack-plugin": "^1.1.12",
    "copy-webpack-plugin": "^6.3.2",
    "crypto": "^1.0.1",
    "debug": "~4.3.1",
    "express": "^4.17.1",
    "http-errors": "~1.8.0",
    "image-webpack-loader": "^7.0.1",
    "jsonwebtoken": "^8.5.1",
    "mime": "^2.4.6",
    "moment": "^2.29.1",
    "multer": "^1.4.2",
    "node-redis": "^0.1.7",
    "node-sass": "^5.0.0",
    "node-tesseract": "^0.2.7",
    "popper.js": "^1.16.1",
    "quill-image-extend-module": "^1.1.2",
    "sass-loader": "^10.1.0",
    "sass-resources-loader": "^2.1.1",
    "svg-captcha": "^1.4.0",
    "view-design": "^4.4.0",
    "vue": "^2.6.12",
    "vue-quill-editor": "^3.0.6",
    "vue-router": "^3.4.9",
    "vuex": "^3.6.0"
  },
  "devDependencies": {
    "babel-core": "7.0.0-bridge.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@vue/cli-plugin-babel": "^4.5.4",
    "@vue/cli-plugin-eslint": "^4.5.4",
    "@vue/cli-plugin-unit-jest": "^3.4.0",
    "@vue/cli-service": "^4.5.9",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-jest": "^23.6.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.13.0",
    "cookie-parser": "^1.4.5",
    "css-loader": "^5.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "fs": "0.0.1-security",
    "fs-extra": "^9.0.1",
    "jest": "^26.6.3",
    "jsdom": "^16.4.0",
    "jsdom-global": "^3.0.2",
    "mysql": "^2.18.1",
    "mysql2": "^2.2.5",
    "path": "^0.12.7",
    "pkg": "^4.4.9",
    "plugin": "^0.3.3",
    "quill": "^1.3.7",
    "quill-image-extend-module": "^1.1.2",
    "redis": "^3.0.2",
    "redis-parser": "^3.0.0",
    "sequelize": "^6.3.5",
    "style-loader": "^2.0.0",
    "tedious": "^9.2.1",
    "url-loader": "^4.1.1",
    "util": "^0.12.3",
    "vue-quill-editor": "^3.0.6",
    "vue-template-compiler": "^2.6.12"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/src/**.{js,vue}",
      "!**/node_modules/**",
      "!**src/main.js**",
      "!**src/router.js**"
    ]
  }
}
